{
  "name": "blhaus",
  "version": "1.0.0",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build": "npm run tailwind:build",
    "tailwind:build": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "postinstall": "echo Postinstall completed successfully",
    "debug": "ls -la && echo 'Node version:' && node --version && echo 'NPM version:' && npm --version && echo 'Directory structure:' && find . -maxdepth 3 -type d"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11"
  }
}
